# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
#GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
#GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
#GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                  controllers.Assets.versioned(path="/public", file: Asset)
# Static resources (not managed by the Play framework)
#GET     /extassets/*file            controllers.Assets.versioned(path="/home/dungvc/Desktop", file: Asset)

#######################################################################

### LOGIN , LOGOUT
GET      /                             controllers.LoginController.index
GET      /login                        controllers.LoginController.login
GET      /logout                       controllers.LoginController.logout

#######################################################################


### PAGE AGE
GET     /internet/age                  churn.controllers.ChurnAgeController.index
POST    /internet/age/getJsonChurn     churn.controllers.ChurnAgeController.getJsonChurn

### PAGE REGION
GET     /internet/region               churn.controllers.ChurnRegionController.index
POST    /internet/region/getJsonChurn  churn.controllers.ChurnRegionController.getJsonChurn

### PAGE CALLLOG
GET     /internet/calllog              churn.controllers.ChurnCallogController.index
POST    /internet/calllog/getJsonChurn churn.controllers.ChurnCallogController.getJsonChurn


