# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
#GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
#GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
#GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                  controllers.Assets.versioned(path="/public", file: Asset)
# Static resources (not managed by the Play framework)
#GET     /extassets/*file            controllers.Assets.versioned(path="/home/dungvc/Desktop", file: Asset)

#######################################################################

### LOGIN , LOGOUT
GET      /                             controllers.LoginController.index
GET      /login                        controllers.LoginController.login
GET      /logout                       controllers.LoginController.logout

#######################################################################


### PROFILE INTERNET
GET     /internet/age                  churn.controllers.ChurnAgeController.index
GET     /internet/getJsonByAge         churn.controllers.ChurnAgeController.getJsonByAge(age: String ?= "")
GET     /internet/getJsonByMonth       churn.controllers.ChurnAgeController.getJsonByMonth(status: String ?= "",month: String?="")
GET     /internet/getJsonByStatus      churn.controllers.ChurnAgeController.getJsonByStatus(status: String ?= "",month: String ?="")
GET     /internet/getChurnByRegionAge  churn.controllers.ChurnAgeController.getChurnByRegionAge(age: String ?= "",region: String ?="")

POST    /internet/getJsonChurn         churn.controllers.ChurnAgeController.getJsonChurn

#######################################################################